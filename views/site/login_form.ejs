<%- include header.ejs %>
<body>
  <form id=form method="post" action="/login_do" >
    <div class="form-group row">
      <div class="col-12 text-center">
        <h2>Autenticação</h2>
      </div>
    </div>
    <div class="form-group row">
      <!-- <label for="titulo" class="col-2 col-form-label">Id</label> -->
      <div class="col-10">
        <input id="user_id" name="user_id" type="hidden" value="<%=user_id%>"  class="form-control">
      </div>
    </div>
    <div class="form-group row">
      <label for="titulo" class="col-2 col-form-label">Usuário</label>
      <div class="col-10">
        <input id="user" name="user" type="text"  required="required" class="form-control">
      </div>
    </div>
    <div class="form-group row">
      <label for="data" class="col-2 col-form-label">Senha</label>
      <div class="col-10">
        <input id="password" name="password" type="text" required="required"  class="form-control">
      </div>
    </div>
    <br>
    <div class="form-group row">
      <div class="offset-2 col-10">
        <button name="submit" type="submit" class="btn btn-primary">Ok</button>
      </div>
    </div>
  </form>
</body>
<%- include footer.ejs %>
